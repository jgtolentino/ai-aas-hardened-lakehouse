{
  "name": "ai-aas-hardened-lakehouse",
  "version": "6.0.0",
  "description": "TBWA Enterprise Data Platform - Unified Scout Dashboard, ETL Pipeline, and AI/MCP Integration",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*",
    "infra/*",
    "components/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean",
    "contracts:gen": "turbo run contracts:gen",
    "figma:parse": "turbo run figma:parse",
    "figma:publish": "turbo run figma:publish",
    "figma:sync": "pnpm figma:parse && pnpm figma:publish",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "mcp:start": "turbo run mcp:start",
    "bruno:test": "./scripts/run_bruno_tests.sh",
    "deploy:scout": "./scripts/deploy-scout-backend.sh",
    "deploy:all": "pnpm build && pnpm deploy:scout",
    "validate:setup": "./scripts/validate-setup.sh",
    "docs:build": "turbo run build --filter=docs-site",
    "docs:dev": "turbo run dev --filter=docs-site"
  },
  "devDependencies": {
    "turbo": "^2.2.3",
    "typescript": "^5.5.4",
    "@types/node": "^20.11.30",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "@figma/code-connect": "^1.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.8",
    "zod": "^3.22.4"
  },
  "packageManager": "pnpm@9.12.4",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "keywords": [
    "scout-dashboard",
    "etl-pipeline", 
    "figma-integration",
    "mcp-bridge",
    "enterprise-data-platform",
    "tbwa"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/scout-db/ai-aas-hardened-lakehouse.git"
  },
  "license": "UNLICENSED"
}