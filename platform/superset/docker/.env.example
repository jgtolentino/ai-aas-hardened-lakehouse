# Superset Environment Variables Example
# Copy this file to .env and update with your actual values

# Database connection
DATABASE_URL=postgresql://scout_viewer:viewer_pass@postgres:5432/scout_analytics

# Redis for caching
REDIS_URL=redis://redis:6379/1

# Secret key for session encryption (generate with: openssl rand -base64 42)
SECRET_KEY=your_secret_key_here

# Mapbox API key for choropleth base maps
# Get your key at: https://www.mapbox.com/
MAPBOX_API_KEY=pk.your_mapbox_api_key_here

# Optional: Custom Mapbox style
MAPBOX_STYLE_URL=mapbox://styles/mapbox/light-v10

# Admin credentials (change these!)
ADMIN_USERNAME=admin
ADMIN_FIRSTNAME=Admin
ADMIN_LASTNAME=User
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=admin

# Scout viewer credentials
SCOUT_VIEWER_USERNAME=scout_viewer
SCOUT_VIEWER_PASSWORD=viewer_pass

# Feature flags
ENABLE_DECK_GL_VISUALIZATIONS=true
ENABLE_GEOSPATIAL_VISUALIZATIONS=true

# Performance settings
SUPERSET_WORKERS=4
SUPERSET_CELERY_WORKERS=2

# Map performance
MAX_GEOMETRY_SIZE_MB=50
MAX_MAP_FEATURES=10000

# Logging
LOG_LEVEL=INFO