{
  "Version": "2012-10-17",
  "Statement": [
    { "Effect": "Allow", "Action": ["s3:ListBucket"],
      "Resource": ["arn:aws:s3:::${bucket_name}"],
      "Condition": {"StringLike": {"s3:prefix": ["gold/*","platinum/*"]}} },
    { "Effect": "Allow", "Action": ["s3:GetObject"],
      "Resource": [
        "arn:aws:s3:::${bucket_name}/gold/*",
        "arn:aws:s3:::${bucket_name}/platinum/*"
      ] }
  ]
}