{
  "Version": "2012-10-17",
  "Statement": [
    { "Effect": "Allow", "Action": ["s3:ListBucket"],
      "Resource": ["arn:aws:s3:::${bucket_name}"],
      "Condition": {"StringLike": {"s3:prefix": ["bronze/*"]}} },
    { "Effect": "Allow", "Action": ["s3:PutObject","s3:AbortMultipartUpload"],
      "Resource": ["arn:aws:s3:::${bucket_name}/bronze/*"] }
  ]
}