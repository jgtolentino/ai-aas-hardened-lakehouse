# Scout Analytics Data Tools Configuration
# Copy to .env and fill in your values

# Supabase connection (for seed_scout_data.py)
SUPABASE_DB_URI=postgresql://postgres:password@db.your-project.supabase.co:5432/postgres

# Alternative database URL (for load_scout_data.sh)
DATABASE_URL=postgresql://postgres:password@db.your-project.supabase.co:5432/postgres

# Optional: Separate read/write connections
DB_WRITE_URL=postgresql://postgres:password@db.your-project.supabase.co:5432/postgres
DB_READ_URL=postgresql://postgres:password@db.your-project.supabase.co:6543/postgres

# Tenant configuration (for RLS)
TBWA_TENANT_ID=your-uuid-here

# Data generation settings
NUM_TRANSACTIONS=18000
NUM_STORES=1299
SEED_START_DATE=2024-01-01

# Market share targets
FMCG_TBWA_SHARE=0.20
TOBACCO_JTI_SHARE=0.39