{
  "name": "Scout Analytics Platform API",
  "entryPoints": [
    "packages/*/src/index.ts",
    "apps/**/src/**/*.ts",
    "agents/**/src/**/*.ts"
  ],
  "out": "docs-site/static/api",
  "excludePrivate": true,
  "excludeInternal": true,
  "skipErrorChecking": true,
  "validation": {
    "notExported": false,
    "invalidLink": false
  },
  "exclude": [
    "**/node_modules/**",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/dist/**",
    "**/build/**",
    "**/worktrees/**",
    "**/.venv/**"
  ]
}